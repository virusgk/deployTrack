<?xml version="1.0" encoding="UTF-8"?>
<configuration>
  <system.webServer>
    <rewrite>
      <rules>
        <!-- This rule forwards all requests for this application to the Next.js server -->
        <rule name="ReverseProxyInbound" stopProcessing="true">
          <match url="(.*)" />
          <action type="Rewrite" url="http://localhost:3000/deploytrack/{R:1}" />
        </rule>
      </rules>
    </rewrite>
  </system.webServer>
</configuration>
